<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<bean id="employee" class="com.ibm.demo.model.Employee">
		<property name="eid">
			<value>101</value>
		</property>
		<property name="firstName">
			<value>Sonu</value>
		</property>
		<property name="salary">
			<value>10.5</value>
		</property>
	</bean>

	<bean id="employee2" class="com.ibm.demo.model.Employee">
	<property name="eid">
			<value>102</value>
		</property>
		<property name="firstName">
			<value>Monu</value>
		</property>
		<property name="salary">
			<value>12.5</value>
		</property>
	</bean>

</beans> 